{
  "name": "@seaofvoices/react-roblox-studio-plugin",
  "version": "0.1.1",
  "description": "React component library to create Roblox plugins",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/seaofvoices/react-roblox-studio-plugin.git"
  },
  "main": "src/init.lua",
  "scripts": {
    "clean": "rm -rf node_modules",
    "format": "stylua .",
    "lint": "sh ./scripts/analyze.sh && selene src",
    "lint:luau": "sh ./scripts/analyze.sh",
    "lint:selene": "selene src",
    "prepare": "npmluau",
    "style-check": "stylua . --check",
    "verify-pack": "yarn pack --dry-run"
  },
  "dependencies": {
    "@jsdotlua/react": "^17.1.0",
    "@jsdotlua/react-roblox": "^17.1.0",
    "luau-signal": "^0.1.0"
  },
  "devDependencies": {
    "npmluau": "^0.1.1"
  },
  "packageManager": "yarn@4.4.1"
}
